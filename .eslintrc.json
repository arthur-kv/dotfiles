{
  "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
  "rules": {
    "no-console": 0,
    "max-len": [
      "error",
      {
        "code": 80,
        "ignoreComments": false,
        "ignoreTrailingComments": false
      }
    ]
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {}
}
